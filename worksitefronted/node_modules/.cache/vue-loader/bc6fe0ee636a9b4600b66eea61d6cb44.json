ck=\"newblog\">\r\n\t\t\t\t<v-icon large>create</v-icon>\r\n\t\t\t\t<span class=\"caption text-lowercase\">Crete</span>\r\n\t\t\t</v-btn>\r\n\t\t</v-row>\r\n\t\t\r\n\r\n\t\t<!-- 列表 -->\r\n\t\t<v-row dense>\r\n\t\t  <v-col v-for=\"(blog,index) in blogs\" :key=\"index\" :cols=\"blog.flex\">\r\n\t\t\t  <v-card>\r\n\t\t\t\t\t<div @click=\"showdetail(blog.article_id)\">\r\n\t\t\t\t<v-lazy>\r\n\t\t\t\t\t<v-img class=\"white--text align-end\" height=\"200px\" src=\"https://cdn.vuetifyjs.com/images/cards/docks.jpg\">\r\n\t\t\t\t\t  <v-card-title>{{blog.title}}</v-card-title>\r\n\t\t\t\t\t</v-img>\r\n\t\t\t\t</v-lazy>\r\n\t\t\t    <v-card-subtitle class=\"pb-0\">{{blog.share_at | dateFormat}} {{blog.article_author}}</v-card-subtitle>\r\n\t\t\t    <v-card-text class=\"text--primary\">\r\n\t\t\t      <div>{{blog.body}}</div>\r\n\t\t\t    </v-card-text>\r\n\t\t\t\t\t</div>\r\n\t\t\t  </v-card>\r\n\t\t  </v-col>\r\n\t\t</v-row>\r\n\t</v-container>\n</template>\n\n<script>\r\n\timport {getbloglist} from '@/network/Blog/netBlog.js'\n\texport default {\n\t\tdata: () => ({\r\n\t\t\tblogs:null\r\n\t\t}),\r\n\t\tcreated(){\r\n\t\t\tgetbloglist().then(res=>{\r\n\t\t\t\tthis.blogs = res.data.results\r\n\t\t\t\tthis.blogs.sort((a,b)=>{return a.flex > b.flex ? -1:1})\r\n\t\t\t}).catch(err => {\r\n\t\t\t\tconsole.log(err)\r\n\t\t\t})\r\n\t\t\t// 整理布局\r\n\t\t},\r\n\t\tmethods:{\r\n\t\t\tshowdetail(id){\r\n\t\t\t\tthis.$router.push({ name: 'BlogDetail', params: { id }})\r\n\t\t\t},\r\n\t\t\tnewblog(){\r\n\t\t\t\tif(!localStorage.getItem('refresh')){\r\n\t\t\t\t\talert('请先登陆吧')\r\n\t\t\t\t}\r\n\t\t\t\telse\r\n\t\t\t\t{\r\n\t\t\t\t\tthis.$router.push({ name: 'NewBlog' })\t\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t}\r\n\t}\n</script>\n\n<style>\r\n\n</style>\n"]}]}